{
  "manifest_version": 3,
  "name": "ClientJS",
  "description": "Envoi les informations vers le Token",
  "version": "2.0",

  "app": {
    "background": {
      "scripts": ["main.js"]
    },
    "sandbox": {
      "pages": ["page.html"]
    }
  },
  "sockets": {
    "tcp": {
      "send": ["*"],
      "connect": ["*"]
    },
    "tcpServer" : {
      "listen": ["*"]
    }
  }
}